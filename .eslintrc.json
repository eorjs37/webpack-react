{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "comma-dangle": "off",
        "indent": "off",
        "no-nested-ternary": "warn",
        "no-unused-vars": "warn",
        "no-undef": "off",
        "vue/comment-directive": 0
    }
}